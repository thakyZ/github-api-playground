  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "${workspaceFolder}/node_modules/**",
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "run",
        "start"
      ],
      "console": "internalConsole",
      "env": {
        "ROARR_LOG": "true"
      }
    }
  ]
}